extensions
[
  csv
]

globals
[
  cell-lifespan               ;; The average life span in tick of cell
  cell-quiescent-count        ;; The total number of cell enter quiscent state occurences
  cell-somatic-spawn-count     ;; The total number of somatic cells that spawn during the simulation
  cell-malignant-spawn-count  ;; The total number of malignant cells that spawn during the simulation
]

;; Define the agentsets and agents for cells.
breed [cells cell]


;; Agent variables that define the cancer and normal cells of the model.
cells-own
[
  malignant?            ;; TRUE, if cell is a cancer cell
  age                   ;; INTEGER, age of cells in hours
  energy                ;; FLOAT, amount of energy the cell has
  low-energy-duration   ;; INTEGER, time in hours the cell has spent at risk of undergoing apoptosis
  proliferation-chance  ;; FLOAT, probability of proliferating at each tick
  quiescent             ;; INTEGER, the duration in tick for the cell to be in quiescent state
  local-artery-density  ;; INTEGER, density of artery patches in a radius of 2 from the nascent cancer cell
]


;; Patches in this model are either Extracellular Matrix (ECM) or artery cells
patches-own
[
  vascular?        ;; TRUE if endemic artery or TRUE if angiogensis occurs on patch
  nutrient         ;; FLOAT, the combined nutrient level on a patch from initialisation and diffusion
                   ;; nutrient represents Oxygen, sugar and other ions that are essential for cells
]


;; Generates the initial vasculature that surrounds the tumour/normal cell milleu. To be viable
;; vasculature, arteries must be contiguous. The vasulature generated by generate-vasculate represents
;; the arteriolar structure that exists at the tumour/normal tissue boundary.
to generate-vasculature
  ask patches [set vascular? false]
  foreach (list min-pxcor 0 max-pxcor) [ x ->
    foreach (list min-pycor 0 max-pycor) [ y ->
      foreach (list min-pzcor 0 max-pzcor) [ z ->
        ask patches with [pycor = y and pzcor = z]
        [
          become-vascular
        ]
        ask patches with [pxcor = x and pzcor = z]
        [
          become-vascular
        ]
        ask patches with [pxcor = x and pycor = y]
        [
          become-vascular
        ]
      ]
    ]
  ]

  ;; Extracellular matrix [ECM] (black patches) begin with a small amount of nutrient to represent baseline levels
  ask patches with [not vascular?] [set nutrient random 3]
  repeat 100 [
    repeat 10 [ replenish-nutrient ]
    diffuse nutrient 0.1
  ]
end


;; Initialises patch variables to represent an artery patch.
;; Artery nutrient level set to 100 to provide sufficient nutrient
;; to the ECM and cells at initialisation. This is to realistically
;; represent ample nutrient cycling through the vascular system.
to become-vascular
  set nutrient 100
  set vascular? true
end


;; Generates the cell numbers and ratio of cancer to normal cells at initialisation.
to generate-cells
  set-default-shape cells "circle"
  set cell-quiescent-count 0
  set cell-somatic-spawn-count 0
  set cell-malignant-spawn-count 0

  ask n-of initial-cells (patches with [not vascular?])
  [
    sprout-cells 1
    [
      set age (1 + random cell-lifespan)
      set energy (3 + random 6)
      set quiescent 0
      ;; cells at initialisation do not count towards local-artery-density
      set local-artery-density -1

      ifelse (random-float 100 <= disease-ratio)
      [
        set malignant? true
        set color green
        set proliferation-chance malignant-proliferation-chance
      ]
      [
        set malignant? false
        set color blue
        set proliferation-chance normal-proliferation-chance
      ]
    ]
  ]
end


;; Increases the ages of all viable cells by one hour for each tick.
to cell-age
  set age age + 1
end

;; Decrease the quiescent duration of cell after each tick.
to cell-quiescent
  if quiescent > 0
  [
    set quiescent (quiescent - 1)
  ]
end


;; Kills cells with a 50% chance once they have been alive for 50 days and not in a quiescent state.
;; Cells can also undergo apoptosis if their energy is too low for too long and not in quiescent state.
;; Malignant cells have a chance to enter a quiescent state and escape death,
;; they can live for a duration without nutrient intake (low energy).
to cell-die
  if quiescent = 0 and (
    (age > cell-lifespan and random 100 < 50)
    or (low-energy-duration > max-low-energy-duration)
  )
  [
    ifelse (malignant? and random 100 < 20)
    [
      set quiescent quiescent-duration
      set cell-quiescent-count (cell-quiescent-count + 1)
    ]
    [
      die
    ]

  ]
end


;; Counts how many hours (ticks) the cell has been with low energy.
;; Increases by 1 every continuous tick with low energy.
;; Reset to 0 once a cell's energy is above a threshold.
to cell-energy-duration
  ifelse(energy < 1)
  [set low-energy-duration low-energy-duration + 1]
  [set low-energy-duration 0]
end


;; Cells absorb nutrients from their local environment (patch-here).
to cell-absorb-energy
  if [nutrient] of patch-here > 1 [
    ask patch-here [set nutrient (nutrient - 1)]
    set energy (energy + 1)
  ]
end


;; Proliferates new cells, if the parent cell has enough energy.
;; Cells cannot proliferate onto an artery patch.
;; Cells cannot proliferate onto a space that is occupied.
to cell-proliferate
  let patch-neighbor one-of neighbors with [not vascular? and not any? cells-here]

  if patch-neighbor != nobody
  [
    if random 100 < proliferation-chance and energy >= energy-req-prolif and quiescent = 0
    [
      set energy (energy / 2)
      hatch 1
      [
        set age 0
        set quiescent 0
        move-to patch-neighbor
        set local-artery-density count (patches with [vascular?] in-radius 2)
        ifelse malignant? = false
        [ set cell-somatic-spawn-count (cell-somatic-spawn-count + 1)]
        [ set cell-malignant-spawn-count (cell-malignant-spawn-count + 1)]
      ]
    ]
  ]
end


;; Visual procedure. Changes the hue of the red vasculature and opacity of the ECM
;; More red hue == more nutrients in artery/arteriole patch
;; More opaque == more nutrients on patch
to show-nutrient
  ask patches with [nutrient > 1 and not vascular?]
  [
    set pcolor (list 255 0 0 (min (list (nutrient) 255)))
  ]
  ask patches with [vascular?]
  [
    set pcolor (list 255 0 0 (min (list (nutrient * 5) 255)))
  ]
end


;; Increases the nutrient level in the vasculature.Nutrients are restored
;; in the vasculature by the cycling blood of the vascular system.
to replenish-nutrient
  ask patches with [vascular?] [
    set nutrient (nutrient + 0.1)
  ]
end

;; Generates vacscular growth in the extracellular matrix if there are enough cancer cells near to
;; existing vasculature. This represents that concentration of growth factor that an artery/arteriole
;; would receive from nearby cancer cells.
;; Only generates vasculature in a contiguous manner, to represent vascular truncation. Neighbours6 is
;; used instead of neighbours to restrict the truncation direction, making more realistic structures.
to angiogenesis
  ask patches with [not vascular? and count neighbors6 with [vascular?] > 0 and count neighbors with [vascular?] < 4] [
    let demand (count cells with [malignant?] in-radius angiogenesis-signal-radius)
    if demand > angiogenesis-min-demand [
      become-vascular
      set nutrient mean [nutrient] of neighbors with [vascular?]
    ]
  ]
end


;; Set-up procedure that instanties the environment and cell agents.
to setup
  clear-all
  reset-ticks
  set cell-lifespan 50
  generate-vasculature
  generate-cells
  show-nutrient
end


;; Go procedure that runs the simulation.
to go

  ;; Stopping conditions: complete cell death or 300 days of growth.
  if count cells = 0 or ticks > 300 [stop]

  ;; nutrients diffusing through the ECM at a constant rate
  diffuse nutrient 0.01

  show-nutrient
  angiogenesis

   ask cells
  [
    cell-quiescent
    cell-die
    cell-age
    cell-absorb-energy
    cell-proliferate
    cell-energy-duration
  ]
  replenish-nutrient
  tick
end

;; data reporting procedures for statistical analysis
to-report count-malignant-cells
  report count cells with [ malignant? ]
end

to-report count-malignant-spawn-cells
  report cell-malignant-spawn-count
end

to-report count-somatic-cells
  report count cells with [ not malignant? ]
end

to-report count-somatic-spawn-cells
  report cell-somatic-spawn-count
end

to-report count-quiescent-cells-occurences
  report cell-quiescent-count
end

to-report count-quiescent-cells-at-final-tick
  report count cells with [ quiescent > 0 ]
end

to-report count-vascular-patches
  report count patches with [ vascular? ]
end


